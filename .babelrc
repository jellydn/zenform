{
  "presets": [
    [
      "env",
      {
        "targets": {
          "ie": 9
        },
        "useBuiltIns": false,
        "modules": false
      }
    ],
    "babel-preset-react"
  ],
  "plugins": ["transform-class-properties", "transform-object-rest-spread"],
  "env": {
    "test": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    },
    "build": {
      "plugins": ["external-helpers"]
    }
  }
}
